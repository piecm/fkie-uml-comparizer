<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_chain" name="ChainModel">
      <UML:Namespace.ownedElement>
        <!-- Class Definitions for A1 to A100 -->
        <!-- Note: This example only includes A1 to A3 for brevity -->

        <UML:Class xmi.id="class_A1" name="A1" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_A1_at" name="at" visibility="public">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <UML:Class xmi.id="class_A2" name="A2" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_A2_at" name="at" visibility="public">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <UML:Class xmi.id="class_A3" name="A3" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_A3_at" name="at" visibility="public">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <!-- Associations -->

        <UML:Association xmi.id="assoc_A1_A2">
          <UML:Association.connection>
            <UML:AssociationEnd xmi.id="assocEnd_A1_A2_A1" type="class_A1" name="A1" visibility="public" aggregation="none" multiplicity="1"/>
            <UML:AssociationEnd xmi.id="assocEnd_A1_A2_A2" type="class_A2" name="A2" visibility="public" aggregation="none" multiplicity="1"/>
          </UML:Association.connection>
        </UML:Association>

        <UML:Association xmi.id="assoc_A2_A3">
          <UML:Association.connection>
            <UML:AssociationEnd xmi.id="assocEnd_A2_A3_A2" type="class_A2" name="A2" visibility="public" aggregation="none" multiplicity="1"/>
            <UML:AssociationEnd xmi.id="assocEnd_A2_A3_A3" type="class_A3" name="A3" visibility="public" aggregation="none" multiplicity="1"/>
          </UML:Association.connection>
        </UML:Association>

        <!-- Note: Associations for A4 to A100 follow the same pattern -->

        <!-- Data Types -->

        <UML:DataType xmi.id="int" name="int"/>

        <!-- Enumeration for OclState -->

        <UML:Enumeration xmi.id="enum_OclState" name="OclState">
          <UML:EnumerationLiteral xmi.id="lit_equal" name="="/>
          <UML:EnumerationLiteral xmi.id="lit_notEqual" name="<>"/>
        </UML:Enumeration>
      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>