<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_LogicalView" name="Logical View">
      <UML:Namespace.ownedElement>
        <!-- Definition der Datentypen -->
        <UML:DataType xmi.id="datatype_int" name="int"/>
        <UML:DataType xmi.id="datatype_char" name="char"/>
        <UML:DataType xmi.id="datatype_bool" name="bool"/>
        <UML:DataType xmi.id="datatype_float" name="float"/>
        <UML:DataType xmi.id="datatype_double" name="double"/>
        <UML:DataType xmi.id="datatype_short" name="short"/>
        <UML:DataType xmi.id="datatype_long" name="long"/>
        <UML:DataType xmi.id="datatype_unsigned_int" name="unsigned int"/>
        <UML:DataType xmi.id="datatype_unsigned_short" name="unsigned short"/>
        <UML:DataType xmi.id="datatype_unsigned_long" name="unsigned long"/>
        <UML:DataType xmi.id="datatype_string" name="string"/>
        <UML:DataType xmi.id="datatype_varchar" name="varchar"/>
        <UML:DataType xmi.id="datatype_tinyint" name="tinyint"/>
        <UML:DataType xmi.id="datatype_smallint" name="smallint"/>
        <UML:DataType xmi.id="datatype_mediumint" name="mediumint"/>
        <UML:DataType xmi.id="datatype_bigint" name="bigint"/>
        <UML:DataType xmi.id="datatype_decimal" name="decimal"/>
        <UML:DataType xmi.id="datatype_date" name="date"/>
        <UML:DataType xmi.id="datatype_datetime" name="datetime"/>
        <UML:DataType xmi.id="datatype_time" name="time"/>
        <UML:DataType xmi.id="datatype_timestamp" name="timestamp"/>
        <UML:DataType xmi.id="datatype_year" name="year"/>
        <UML:DataType xmi.id="datatype_tinyblob" name="tinyblob"/>
        <UML:DataType xmi.id="datatype_blob" name="blob"/>
        <UML:DataType xmi.id="datatype_mediumblob" name="mediumblob"/>
        <UML:DataType xmi.id="datatype_longblob" name="longblob"/>
        <UML:DataType xmi.id="datatype_tinytext" name="tinytext"/>
        <UML:DataType xmi.id="datatype_text" name="text"/>
        <UML:DataType xmi.id="datatype_mediumtext" name="mediumtext"/>
        <UML:DataType xmi.id="datatype_longtext" name="longtext"/>
        <UML:DataType xmi.id="datatype_enum" name="enum"/>
        <UML:DataType xmi.id="datatype_set" name="set"/>

        <!-- Definition der Klassen -->
        <UML:Class xmi.id="class_posts" name="posts" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_posts_id" name="id" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_posts_title" name="title" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_posts_content" name="content" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_mediumtext"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_posts_summary" name="summary" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_text"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_posts_date_creation" name="date_creation" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_date"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <UML:Class xmi.id="class_editors" name="editors" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_editors_id" name="id" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_editors_name" name="name" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_editors_password" name="password" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_editors_mail" name="mail" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_editors_date_registration" name="date_registration" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_datetime"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_editors_public" name="public" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_bool"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <UML:Class xmi.id="class_categories" name="categories" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_categories_id" name="id" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_categories_name" name="name" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <UML:Class xmi.id="class_comments" name="comments" visibility="public" isAbstract="false">
          <UML:Namespace.ownedElement>
            <UML:Attribute xmi.id="attr_comments_id" name="id" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_int"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_comments_author" name="author" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_comments_content" name="content" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_text"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_comments_date_creation" name="date_creation" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_datetime"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
            <UML:Attribute xmi.id="attr_comments_mail" name="mail" visibility="private">
              <UML:StructuralFeature.type>
                <UML:DataType xmi.idref="datatype_varchar"/>
              </UML:StructuralFeature.type>
            </UML:Attribute>
          </UML:Namespace.ownedElement>
        </UML:Class>

        <!-- Definition der Assoziationen -->
        <UML:Association xmi.id="assoc_posts_comments">
          <UML:Association.connection>
            <UML:AssociationEnd xmi.id="assocEnd_posts_comments_posts" type="class_posts" name="post" visibility="public" aggregation="none" multiplicity="1"/>
            <UML:AssociationEnd xmi.id="assocEnd_posts_comments_comments" type="class_comments" name="comment" visibility="public" aggregation="none" multiplicity="0..*"/>
          </UML:Association.connection>
        </UML:Association>

        <UML:Association xmi.id="assoc_posts_editors">
          <UML:Association.connection>
            <UML:AssociationEnd xmi.id="assocEnd_posts_editors_posts" type="class_posts" name="post" visibility="public" aggregation="none" multiplicity="0..*"/>
            <UML:AssociationEnd xmi.id="assocEnd_posts_editors_editors" type="class_editors" name="editor" visibility="public" aggregation="none" multiplicity="1"/>
          </UML:Association.connection>
        </UML:Association>

        <UML:Association xmi.id="assoc_posts_categories">
          <UML:Association.connection>
            <UML:AssociationEnd xmi.id="assocEnd_posts_categories_posts" type="class_posts" name="post" visibility="public" aggregation="none" multiplicity="0..*"/>
            <UML:AssociationEnd xmi.id="assocEnd_posts_categories_categories" type="class_categories" name="category" visibility="public" aggregation="none" multiplicity="0..*"/>
          </UML:Association.connection>
        </UML:Association>
      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>