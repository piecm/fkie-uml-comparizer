<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>ArgoUML</XMI.exporter>
      <XMI.exporterVersion>0.34</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_IFMLProfile" name="IFML-Profile">
      <UML:Namespace.ownedElement>

        <!-- Hauptklasse IFML-Profile -->
        <UML:Package xmi.id="package_IFMLProfile" name="IFML-Profile">
          <UML:Namespace.ownedElement>

            <!-- IFML-Profile-Extensions -->
            <UML:Package xmi.id="package_Extensions" name="IFML-Profile-Extensions">
              <UML:Namespace.ownedElement>
                
                <!-- Stereotyp ValidationRule -->
                <UML:Stereotype xmi.id="stereotype_ValidationRule" name="ValidationRule" baseClass="OpaqueExpression"/>

                <!-- Stereotyp Field -->
                <UML:Stereotype xmi.id="stereotype_Field" name="Field" baseClass="Component"/>

                <!-- Stereotyp SelectEvent -->
                <UML:Stereotype xmi.id="stereotype_SelectEvent" name="SelectEvent"/>

                <!-- Stereotyp Window mit Attributen -->
                <UML:Stereotype xmi.id="stereotype_Window" name="Window" baseClass="Component">
                  <UML:Classifier.feature>
                    <UML:Attribute xmi.id="attribute_isNewWindow" name="isNewWindow" visibility="public" isSpecification="false"/>
                    <UML:Attribute xmi.id="attribute_isModal" name="isModal" visibility="public" isSpecification="false"/>
                  </UML:Classifier.feature>
                </UML:Stereotype>

              </UML:Namespace.ownedElement>
            </UML:Package>

            <!-- IFML-Profile-Core -->
            <UML:Package xmi.id="package_Core" name="IFML-Profile-Core">
              <UML:Namespace.ownedElement>
                
                <!-- Stereotyp ViewComponent -->
                <UML:Stereotype xmi.id="stereotype_ViewComponent" name="ViewComponent"/>

                <!-- Abstrakter Stereotyp Event -->
                <UML:Stereotype xmi.id="stereotype_Event" name="Event" isAbstract="true"/>

                <!-- Abstrakter Stereotyp InteractionFlowElement -->
                <UML:Stereotype xmi.id="stereotype_InteractionFlowElement" name="InteractionFlowElement" isAbstract="true"/>

              </UML:Namespace.ownedElement>
            </UML:Package>

          </UML:Namespace.ownedElement>
          
          <!-- Paketimporte -->
          <UML:PackageImport>
            <UML:Package xmi.idref="uml_Specifications"/>
          </UML:PackageImport>

        </UML:Package>

      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>