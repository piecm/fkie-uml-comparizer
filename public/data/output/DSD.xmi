<?xml version="1.0" encoding="UTF-8"?>
<XMI xmi.version="1.2" xmlns:UML="org.omg.xmi.namespace.UML">
  <XMI.header>
    <XMI.documentation>
      <XMI.exporter>YourToolName</XMI.exporter>
      <XMI.exporterVersion>YourVersion</XMI.exporterVersion>
    </XMI.documentation>
    <XMI.metamodel xmi.name="UML" xmi.version="1.4"/>
  </XMI.header>
  <XMI.content>
    <UML:Model xmi.id="model_LogicalView" name="Logical View">
      <UML:Namespace.ownedElement>
        <UML:Package xmi.id="package_LogicalModel" name="Logical Model">
          <UML:Namespace.ownedElement>
            <UML:Package xmi.id="package_gov" name="gov">
              <UML:Namespace.ownedElement>
                <UML:Package xmi.id="package_nih" name="nih">
                  <UML:Namespace.ownedElement>
                    <UML:Package xmi.id="package_nci" name="nci">
                      <UML:Namespace.ownedElement>
                        <UML:Package xmi.id="package_ivi" name="ivi">
                          <UML:Namespace.ownedElement>
                            <UML:Package xmi.id="package_bdt" name="bdt">
                              <UML:Namespace.ownedElement>
                                <UML:Class xmi.id="class_DataLocation" name="DataLocation" visibility="public" isAbstract="false">
                                  <UML:Namespace.ownedElement>
                                    <UML:Attribute xmi.id="attr_compressionType" name="compressionType" visibility="private"/>
                                    <UML:Attribute xmi.id="attr_id" name="id" visibility="private"/>
                                    <UML:Attribute xmi.id="attr_locationPort" name="locationPort" visibility="private"/>
                                    <UML:Attribute xmi.id="attr_locationURL" name="locationURL" visibility="private"/>
                                    <UML:Attribute xmi.id="attr_transportMechanismType" name="transportMechanismType" visibility="private"/>
                                  </UML:Namespace.ownedElement>
                                </UML:Class>
                              </UML:Namespace.ownedElement>
                            </UML:Package>
                          </UML:Namespace.ownedElement>
                        </UML:Package>
                      </UML:Namespace.ownedElement>
                    </UML:Package>
                  </UML:Namespace.ownedElement>
                </UML:Package>
              </UML:Namespace.ownedElement>
            </UML:Package>
          </UML:Namespace.ownedElement>
        </UML:Package>
        <UML:Package xmi.id="package_dsd" name="dsd">
          <UML:Namespace.ownedElement>
            <UML:Class xmi.id="class_Package" name="Package" visibility="public" isAbstract="false">
              <UML:Namespace.ownedElement>
                <UML:Attribute xmi.id="attr_id" name="id" visibility="private"/>
                <UML:Attribute xmi.id="attr_identifier" name="identifier" visibility="private"/>
                <UML:Attribute xmi.id="attr_majorVersion" name="majorVersion" visibility="private"/>
                <UML:Attribute xmi.id="attr_minorVersion" name="minorVersion" visibility="private"/>
                <UML:Attribute xmi.id="attr_name" name="name" visibility="private"/>
              </UML:Namespace.ownedElement>
            </UML:Class>
            <UML:Class xmi.id="class_ContainerInfo" name="ContainerInfo" visibility="public" isAbstract="false">
              <UML:Namespace.ownedElement>
                <!-- Attributes like cpu and gpu info can be defined here -->
              </UML:Namespace.ownedElement>
            </UML:Class>
            <UML:Class xmi.id="class_URL" name="URL" visibility="public" isAbstract="false">
              <UML:Namespace.ownedElement>
                <UML:Attribute xmi.id="attr_id" name="id" visibility="private"/>
                <UML:Attribute xmi.id="attr_value" name="value" visibility="private"/>
              </UML:Namespace.ownedElement>
            </UML:Class>
          </UML:Namespace.ownedElement>
        </UML:Package>
        <UML:Package xmi.id="package_java" name="java">
          <UML:Namespace.ownedElement>
            <UML:Package xmi.id="package_lang" name="lang">
              <UML:Namespace.ownedElement>
                <UML:Class xmi.id="class_Integer" name="Integer" visibility="public" isAbstract="false"/>
                <UML:Class xmi.id="class_String" name="String" visibility="public" isAbstract="false"/>
                <UML:Class xmi.id="class_Long" name="Long" visibility="public" isAbstract="false"/>
                <UML:Class xmi.id="class_Boolean" name="Boolean" visibility="public" isAbstract="false"/>
                <UML:Class xmi.id="class_Float" name="Float" visibility="public" isAbstract="false"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
            <UML:Package xmi.id="package_util" name="util">
              <UML:Namespace.ownedElement>
                <UML:Class xmi.id="class_Date" name="Date" visibility="public" isAbstract="false"/>
              </UML:Namespace.ownedElement>
            </UML:Package>
          </UML:Namespace.ownedElement>
        </UML:Package>
      </UML:Namespace.ownedElement>
    </UML:Model>
  </XMI.content>
</XMI>